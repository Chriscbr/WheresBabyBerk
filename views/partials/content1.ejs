<% if (!content1.found) { %>
	<h4 class="text-center extra-space">No BabyBerk found. Check the latest tweets below!</h4>
<% } else { %>
	<% if (content1.early) { %>
		<h4 class="text-center extra-space">Going to <u><%= content1.places[0] %></u> at <u><%= content1.times[0] %></u></h4>
	<% } else { %>
		<h4 class="text-center extra-space">Currently at <u><%= content1.places[0] %></u> until <u><%= content1.times[1] %></u></h4>
	<% } %>
	<% if (content1.places.length > 1) { %>
		<table class="table">
		<thead>
			<tr>
				<th class="text-center">Location</th>
				<th class="text-center">Time</th>
			</tr>
		</thead>
		<tbody>
		<% for(var i=1; i<content1.places.length; i++) { %>
			<tr>
			<td class="text-center"><%= content1.places[i] %></td>
			<td class="text-center"><%= content1.times[2*i] %>-<%= content1.times[2*i+1] %></td>
			</tr>
		<% } %>
		</tbody>
		</table>
	<% } %>
<% } %>